{
    "env": {
       "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:import/errors",
        "plugin:import/warnings",
        "google",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins":[
        "prettier"
    ],
    "rules": {
        "require-jsdoc":"off"
    }
}
